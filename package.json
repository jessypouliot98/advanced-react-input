{
  "name": "advanced-react-input",
  "version": "3.1.1",
  "description": "A scalable TypeScript React component from inputs",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss -o ./lib/style.css ./src/style.css",
    "build:js": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "keywords": [
    "typescript",
    "react",
    "input",
    "javascript",
    "tailwindcss"
  ],
  "author": "JessyP98",
  "repository": "github:jessypouliot98/advanced-react-input",
  "bugs": {
    "url": "https://github.com/jessypouliot98/advanced-react-input/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.4",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
